 index="10" value="103"/><column index="11" value="57"/></column_widths><filter_values><column index="8" value="0"/></filter_values><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">CREATE VIEW WorksWithReadStatus AS
SELECT
    W.WorkID,
    W.Name,
    W.Series,
    W.NumberInSeries,
    W.Type,
    W.PublishedOn,
	W.Genre,
	W.GoodreadsScore,
    CASE 
        WHEN R.WorkID IS NOT NULL THEN 1  -- or TRUE
        ELSE 0                            -- or FALSE
    END AS IsRead
FROM WORKS W
LEFT JOIN READS R ON R.WorkID = W.WorkID;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
